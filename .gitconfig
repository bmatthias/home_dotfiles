[user]
	name = Michal Papis
	email = mpapis@gmail.com
[dir]
[color]
	ui = auto
[core]
	pager = catless -RS
[alias]
	sync = !sh -c \"git push wayne master && git push origin master && git checkout stable && git push wayne stable && git push origin stable && git checkout master && git push wayne --tags\"
[projects]
	root = $HOME/projects
	depth = 2
[projects "day"]
	start = +3hour
	end = +8hour +1day
[push]
	default = current
